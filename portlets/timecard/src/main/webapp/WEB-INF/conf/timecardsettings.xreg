<registry>
    <portlet-entry name="TimecardSettings" hidden="false" type="ref" parent="Velocity" application="false">
        <security-ref parent="admin-view"/>
        <meta-info>
            <title>타임카드관리(미니버전)</title>
            <description>타임카드(미니버전)의 근태시간을 관리합니다.</description>
        </meta-info>
        <classname>org.apache.jetspeed.portal.portlets.VelocityPortlet</classname>
        <parameter name="template" value="timecardsettings-detail" hidden="true" cachedOnName="true" cachedOnValue="true"/>
        <parameter name="action" value="timecard.TimecardSettingsAction" hidden="true" cachedOnName="true" cachedOnValue="true"/>
        <parameter name="_showtitlebar" value="false" hidden="true" cachedOnName="true" cachedOnValue="true"/>
        <media-type ref="html"/>
        <url cachedOnURL="true"/>
    </portlet-entry>

</registry>
