        <parameter name="pba-template.style.values" value="블록,일,주,월,목록" hidden="true" cachedOnName="true" cachedOnValue="true"/>
        <parameter name="p19-rows" value="detail" type="style" hidden="false" cachedOnName="true" cachedOnValue="true">
	        <meta-info>
                <title>표시형식（첫페이지）</title>
            </meta-info>
    	</parameter>
    	<parameter name="p19-rows.style" value="ListBox" hidden="true" cachedOnName="true" cachedOnValue="true"/>
